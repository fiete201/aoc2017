#!/usr/bin/python3
# Imports
import sys
import os
sys.path.append(os.path.join(os.path.dirname(__file__), '../'))
import day01
import day02
import day04
import day05
import day06
# Global variables
# Functions

# Main
if __name__ == '__main__':
    i = 1
    max_i = len(sys.argv)
    while i < max_i:
        if i == 1:
            day01.bench(1, sys.argv[i])
        elif i == 2:
            day01.bench(2, sys.argv[i])
        elif i == 3:
            day02.bench(1, sys.argv[i])
        elif i == 4:
            day02.bench(2, sys.argv[i])
        elif i == 5:
            print('skip')
        elif i == 6:
            print('skip')
        elif i == 7:
            day04.bench(1, sys.argv[i])
        elif i == 8:
            day04.bench(2, sys.argv[i])
        elif i == 9:
            day05.bench(1, sys.argv[i])
        elif i == 10:
            day05.bench(2, sys.argv[i])
        elif i == 11:
            day06.bench(1, sys.argv[i])
        elif i == 12:
            day06.bench(2, sys.argv[i])
        else:
            print('skip')
        i += 1
