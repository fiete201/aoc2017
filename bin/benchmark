    while i < max_i:
        if i == 0:
            next
        elif i == 1:
            print('skip')
        elif i == 2:
            print('skip')
        elif i == 3:
            print('skip')
        elif i == 4:
            print('skip')
        elif i == 5:
            print('skip')
        elif i == 6:
            print('skip')
